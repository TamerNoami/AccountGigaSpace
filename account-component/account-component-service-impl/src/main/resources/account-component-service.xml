<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="accountComponentService" class="se.lexicon.account.component.service.AccountComponentServiceImpl">
        <constructor-arg name="accountDao" ref="accountDao"/>
    </bean>

    <bean id="bookOrderComponentService" class="se.lexicon.account.component.service.BookOrderComponentServiceImpl">
        <constructor-arg name="bookOrderDao" ref="bookOrderDao"/>
    </bean>


</beans>
